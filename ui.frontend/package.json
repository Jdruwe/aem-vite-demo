{
  "name": "aem-maven-archetype",
  "version": "1.0.0",
  "description": "Generates an AEM Frontend project with Webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/aem-project-archetype"
  },
  "private": true,
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "dev": "concurrently \"vite -c vite.config.js\" \"vite -c other.vite.config.js\"",
    "type-check": "vue-tsc --noEmit",
    "create-clientlibs": "clientlib --verbose",
    "build-vite": "vite build -c vite.config.js && vite build -c other.vite.config.js",
    "prod": "yarn run type-check && yarn run build-vite && yarn run create-clientlibs"
  },
  "devDependencies": {
    "@jeroendruwe/aem-clientlib-generator": "^1.7.8",
    "@vitejs/plugin-vue": "^1.9.3",
    "concurrently": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.19.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.5",
    "vue-tsc": "^0.28.3"
  },
  "dependencies": {
    "vue": "^3.2.20"
  },
  "browserslist": [
    "last 2 version",
    "> 1%"
  ]
}
